<Window x:Class="PknuWpf.SecondWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PknuWpf"
        mc:Ignorable="d"
        Title="SecondWindow" Height="720" Width="1280">
    <StackPanel>
        <Image Height="202" Source="/Media/cat.jpg" Stretch="UniformToFill" Width="199"/>
        <MediaElement Height="300" Source="Media/cat.wmv" Stretch="None" HorizontalAlignment="Center" Width="600" LoadedBehavior="Play"/>
    </StackPanel>
</Window>
